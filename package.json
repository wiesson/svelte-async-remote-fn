		"@sveltejs/adapter-vercel": "^6.3.1",
		"@sveltejs/kit": "^2.52.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.2.3",
		"clsx": "^2.1.1",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"mode-watcher": "^1.1.0",
