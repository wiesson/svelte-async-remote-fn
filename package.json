{
	"name": "svelte-async-remote-fn",
	"version": "1.0.0",
	"description": "Interactive showcase of SvelteKit Remote Functions patterns - query, command, and form patterns with live examples",
	"type": "module",
	"keywords": [
		"sveltekit",
		"svelte",
		"remote-functions",
		"async",
		"demo",
		"query",
		"command",
		"form",
		"shadcn-svelte",
		"tailwindcss"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/wiesson/svelte-async-remote-fn"
	},
	"bugs": {
		"url": "https://github.com/wiesson/svelte-async-remote-fn/issues"
	},
	"homepage": "https://svelte-async-remote-fn.vercel.app",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
